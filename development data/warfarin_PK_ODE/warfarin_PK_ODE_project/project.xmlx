		<data columnDelimiter="," headers="ID,TIME,COV,DOSE,YTYPE,Y,MDV,COV" uri="/home/blaudez/workspace/lixoft/ddmore/lixoftWP5/converter/scripts/testdir/warfarin_PK_ODE/warfarin_PK_ODE_data.txt"/>
		<models>
			<statisticalModels>
				<parameterList>
					<parameter name="V" transformation="L">
						<intercept estimationMethod="MLE" initialization="10"/>
						<variability estimationMethod="MLE" initialization="1" level="1" levelName="IIV"/>
					</parameter>
					<parameter name="KA" transformation="L">
						<intercept estimationMethod="MLE" initialization="5"/>
						<variability estimationMethod="MLE" initialization="1" level="1" levelName="IIV"/>
					</parameter>
					<parameter name="CL" transformation="L">
						<intercept estimationMethod="MLE" initialization="5"/>
						<variability estimationMethod="MLE" initialization="1" level="1" levelName="IIV"/>
					</parameter>
					<parameter name="TLAG" transformation="L">
						<intercept estimationMethod="MLE" initialization="5"/>
						<variability estimationMethod="MLE" initialization="1" level="1" levelName="IIV"/>
					</parameter>
				</parameterList>
				<correlationModelList>
					<correlationModel level="1" levelName="IIV">
						<blockList>
							<block parameters="V,CL"/>
						</blockList>
					</correlationModel>
				</correlationModelList>
				<observationModelList>
					<observationModel name="Y" outputId="1" pred="CC" type="continuous">
